    "@nestjs/testing": "6.5.2",
    "@nestjs/typeorm": "6.1.3",
    "@types/fs-extra": "^7.0.0",
    "@vendure/common": "^0.4.0",
    "apollo-server-express": "^2.4.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
