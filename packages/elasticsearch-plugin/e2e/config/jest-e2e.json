{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testEnvironment": "node",
  "globals": {
    "ts-jest": {
      "tsConfig": "<rootDir>/config/tsconfig.e2e.json",
      "diagnostics": false
    }
  }
}
