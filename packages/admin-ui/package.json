    "jasmine-spec-reporter": "~5.0.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.2",
    "puppeteer": "^2.1.1",
