    "@webcomponents/custom-elements": "^1.2.4",
    "apollo-angular": "^1.8.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-upload-client": "^12.1.0",
    "core-js": "^3.1.3",
    "dayjs": "^1.8.20",
