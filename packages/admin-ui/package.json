    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.5",
    "@angular-devkit/build-ng-packagr": "~0.900.5",
    "@angular/cli": "^9.0.5",
    "@angular/compiler": "^9.0.6",
    "@angular/compiler-cli": "^9.0.6",
    "@biesbjerg/ngx-translate-extract": "^6.0.3",
