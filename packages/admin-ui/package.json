{
  "name": "@vendure/admin-ui",
  "version": "0.11.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node scripts/set-version.js && ng serve",
    "build:app": "ng build --prod",
    "build": "node scripts/copy-package-json.js && node scripts/set-version.js && node scripts/build-public-api.js && ng build vendure-admin-lib --prod && node scripts/compile-styles.js",
    "watch": "ng build --watch=true",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCI --progress=false",
    "lint": "tslint --fix",
    "extract-translations": "ngx-translate-extract --input ./src --output ./src/lib/static/i18n-messages/en.json --clean --sort --format namespaced-json --format-indentation \"  \" -m _",
    "ngcc": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "publishConfig": {
    "access": "public",
    "directory": "package"
  },
  "dependencies": {
    "@angular/animations": "9.1.0",
    "@angular/cdk": "9.2.0",
